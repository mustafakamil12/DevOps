result = " | ".join(
    x for x in gpuDeviceModel 
    if isinstance(x, str) and x.strip() and x.strip() != "[None]"
)


def clean_gpu_list(raw_list):
    if not isinstance(raw_list, list):
        return []
    return [
        x for x in raw_list
        if isinstance(x, str) and x.strip() and x.strip() != "[None]"
    ]
